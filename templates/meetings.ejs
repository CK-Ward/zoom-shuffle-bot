<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Zoom meetings</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <style>
        body {
            padding-top: 20px;
            padding-bottom: 20px;
        }
    </style>
</head>
<body>
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1>Meetings</h1>
            <div class="list-group">
                <% meetings.forEach((meeting) => { %>
                    <a href="/meetings/<%= meeting.id %>/attendees" class="list-group-item">
                        <h4 class="list-group-item-heading"><%= meeting.name %></h4>
                    </a>
                <% }) %>
            </div>
        </div>
    </div>
</div>
</body>
</html>